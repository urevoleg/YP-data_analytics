   "source": [
    "def for_plot(data, x, y):\n",
    "    \"\"\"\"призвана строить гистограммы в единообразном стиле\"\"\"\n",
    "    sns.set_palette('pastel')\n",
    "    plt.figure(figsize=(18,4))\n",
    "    sns.barplot(x = x, y = y, data = data)\n",
    "    plt.xlabel(x)\n",
    "    plt.ylabel(y)\n",
    "    plt.palette = 'mate'\n",
    "    plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [],
   "source": [
    "game_stat = game_industry.pivot_table(index = 'year_of_release', values = 'name', aggfunc = 'count').reset_index()"
   ]
  },
